.sf-menu,.sf-menu * {
	list-style: none;
	margin: 0;
	padding: 0;
}

.sf-menu {
	float: left;
}

.sf-menu li {
	background-color: #F5F5F5;
	float: left;
	position: relative;
	z-index: 1;
	margin-right: 15px;
}

.sf-menu>li {
	border-radius: 1px;
}

.sf-menu ul {
	position: absolute;
	top: -999em;
	text-align: left;
	border-top: 1px solid #5f636a;
}

.sf-menu ul li {
	background-color: #fff;
	margin-right: 0;
}

.sf-menu li:hover {
	visibility: inherit;
	background-color: #eef4ff;
}

.sf-menu li ul li:hover {
	background-color: #eef4ff !important;
}

.sf-menu li ul li:hover>a {
	color: #3c9a3a !important;
}

.sf-menu a {
	display: block;
	position: relative;
	text-decoration: none;
}

.sf-menu li:hover ul,.sf-menu li.sfHover ul {
	top: 31px;
	z-index: 199;
}

.sf-menu li a {
	color: #3c9a3a;
	text-decoration: none;
	padding: 5px 20px;
}

.sf-menu li li:hover ul,ul.sf-menu li li.sfHover ul {
	left: 14.7em;
	top: -1.7em;
}

.sf-menu li li li:hover ul,ul.sf-menu li li li.sfHover ul {
	left: 11.3em;
	top: 0;
}

ul.sf-menu li a:hover {
	color: #3c9a3a;
	text-decoration: none;
}

.sf-menu li ul li a {
	font-size: 12px !important;
	text-transform: capitalize !important;
	width: 140px;
	color: #868787;
	padding: 5px 10px;
}

.sf-menu ul li a:focus,.sf-menu ul li a:hover,.sf-menu ul li a:active {
	outline: 0;
	color: #4d4c4c !important;
}

.sf-menu li ul li {
	border: 1px solid #5f636a;
	border-top: none;
	width: 158px;
}

.sf-menu li ul li ul {
	margin-left: -10px;
	margin-top: 15px;
}

.sf-menu li ul li,.sf-menu li {
	-webkit-transition: all .2s linear;
	-moz-transition: all .2s linear;
	-ms-transition: all .2s linear;
	-o-transition: all .2s linear;
	transition: all .2s linear;
}

.fix-fish-menu select {
	display: none;
	width: 100%;
	height: 28px;
	margin: 21px 0;
}

.sf-sub-indicator {
	position: absolute;
	display: block;
	right: 27px;
	top: 8px;
	width: 10px;
	height: 20px;
	text-indent: -999em;
	overflow: hidden;
	background: url(../../img/tool/ico/sf-arrows.png) no-repeat -10px
		-100px;
}

a>.sf-sub-indicator {
	top: 8px;
	background-position: 0 -100px;
}

a:focus>.sf-sub-indicator,a:hover>.sf-sub-indicator,a:active>.sf-sub-indicator,li:hover>a>.sf-sub-indicator,li.sfHover>a>.sf-sub-indicator
	{
	background-position: -10px -100px;
}

.sf-menu ul a>.sf-sub-indicator {
	background-position: 0 0;
}

.sf-menu li:hover li ul,ul.sf-menu li.sfHover li ul,.sf-menu li li:hover li ul,ul.sf-menu li li.sfHover li ul
	{
	top: -999em;
	z-index: 9999;
}

ul.sf-menu li.active,.sf-menu li ul li:hover,.sf-menu li:hover {
	background-color: #eef4ff;
}

.sf-menu ul .sf-sub-indicator,.sf-menu ul a:focus>.sf-sub-indicator,.sf-menu ul a:hover>.sf-sub-indicator,.sf-menu ul a:active>.sf-sub-indicator,.sf-menu ul li:hover>a>.sf-sub-indicator,.sf-menu ul li.sfHover>a>.sf-sub-indicator
	{
	background-position: -10px 0;
}

.tipsy {
